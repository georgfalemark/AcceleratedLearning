@model AddRoleVm

<h1>All users in @Model.Role</h1>

<table class="table">
    <tr>
        <th>Email</th>
    
        <th>User Name</th>
        
        <th>User Id</th>
    
    </tr>
    <tbody>
        @foreach (var item in Model.indentityusers)
        {
            <tr>
                <td>@item.Email</td>
            
                <td>@item.UserName</td>
            
                <td>
                    @*<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |*@
                    <a asp-controller="Admin" asp-action="DeleteUserById" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>

        }
    </tbody>
</table>

